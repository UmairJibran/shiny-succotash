<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.umairjibran.mobiledevelopmentlabfinalexam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileDevelopmentLabFinalExam">
        <activity
            android:name=".ui.login.AdminLogin"
            android:label="@string/title_activity_admin_login"></activity>
        <activity android:name=".UserHomeScreen" />
        <activity
            android:name=".ui.login.RegisterActivity"
            android:label="@string/user_register_title" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/user_login_activity" />
    </application>

</manifest>